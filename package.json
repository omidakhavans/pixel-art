{
  "name": "@rbl/pixel-art",
  "version": "1.0.0",
  "private": true,
  "description": "Draw and display pixel art on your website.",
  "author": "RebelCode",
  "license": "GPL-2.0-or-later",
  "main": "build/block/block.js",
  "engines": {
    "node": "18",
    "npm": ">=10"
  },
  "scripts": {
    "build": "wp-scripts build --config ./webpack.config.js",
    "format": "wp-scripts format",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "lint:js:fix": "wp-scripts lint-js --fix",
    "lint:php": "composer run lint",
    "lint": "npm run lint:js && npm run lint:php && npm run lint:css",
    "packages-update": "wp-scripts packages-update",
    "start": "wp-scripts start --config ./webpack.config.js"
  },
  "devDependencies": {
    "@wordpress/prettier-config": "2.1.0",
    "@wordpress/scripts": "^28.0.0",
    "@wordpress/stylelint-config": "21.1.0",
    "eslint": "8.23.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "path-browserify": "^1.0.1"
  },
  "dependencies": {
    "@wordpress/api-fetch": "^6.55.0",
    "@wordpress/block-editor": "^13.0.0",
    "@wordpress/blocks": "^13.0.0",
    "@wordpress/components": "^28.0.0",
    "@wordpress/i18n": "^5.0.0",
    "@wordpress/url": "^3.59.0"
  }
}
